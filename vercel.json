{
    "version": 2,
    "public": true,
    "name": "eco-ride",
    "builds": [
      {
        "src": "frontend/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/frontend/css/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/html/$1"
      }
    ]
  }
  